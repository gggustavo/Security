@using MSA.Models;
@using Microsoft.AspNet.Identity;
@{
    ViewBag.Title = "Administrar cuenta";
}

@if (ViewBag.UserNotification != null)
{
    <br />
    <div class="bb-alert alert alert-success">@ViewBag.UserNotification</div>
}
@if (ViewBag.UserNotFound != null)
{
    <br />
    <div class="bb-alert alert alert-danger">@ViewBag.UserNotFound</div>
}


<div class="row">
    <div class="col-md-12">
        @Html.Partial("_ChangePasswordUserPartial")
    </div>
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}


